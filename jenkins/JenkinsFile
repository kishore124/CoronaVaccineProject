pipeline{
    agent any
    stages{
        stage("build"){
            steps{
                    sh "mvn clean package"
            }
        }
        stage("test"){
            steps{
                    sh "mvn test"
            }
        }
        stage("deliver"){
            steps{
                   echo "Code is delivered"
            }
        }
       stage("completed"){
            steps{
                   echo "Code is completed"
            }
        }
    }
    
}
